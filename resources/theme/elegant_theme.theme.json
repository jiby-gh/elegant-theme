{
  "name": "Elegant Theme",
  "dark": true,
  "author": "yx208",
  "editorScheme": "/theme/elegant_theme.icls",
  "colors": {
    "dark": "#26292E",
    "gray": "#353a42",
    "gray2": "#2d3138",
    "primary": "#085dc2",
    "primarySecond": "#2077f1",
    "white": "#e5e5e5",
    "white2": "#A9B7C6"
  },
  "ui": {
    "*": {
      "background": "dark",
      "foreground": "white",
      "borderColor": "gray",
      "selectionBackground": "gray",
      "separatorColor": "gray",
      "selectionInactiveBackground": "gray",
      "selectionBackgroundInactive": "gray"
    },
    "CheckBoxMenuItem.selectionBackground": "primary",
    "Button": {
      "default": {
        "foreground": "white",
        "startBackground": "primary",
        "endBackground": "primarySecond",
        "startBorderColor": "primary",
        "endBorderColor": "primarySecond",
        "focusedBorderColor": "white"
      }
    },
    "Tree": {
      "selectionBackground": "gray",
      "selectionInactiveBackground": "gray",
      "selectionForeground": "white",
      "rowHeight": 26
    },
    "EditorPane.inactiveBackground": "dark",
    "CompletionPopup": {
      "foreground": "white2",
      "matchForeground": "primarySecond",
      "selectionBackground": "gray2",
      "selectionInactiveBackground": "gray2"
    },
    "EditorTabs": {
      "underlineColor": "primarySecond",
      "inactiveUnderlineColor": "primarySecond",
      "underlineHeight": 2,
      "underlinedTabBackground": "gray"
    },
    "ToolWindow": {
      "Header": {
        "background": "dark",
        "inactiveBackground": "dark"
      }
    },
    "WelcomeScreen.separatorColor": "gray",
    "DefaultTabs": {
      "hoverBackground": "gray",
      "underlineColor": "primarySecond",
      "underlineHeight": 2
    },
    "TabbedPane": {
      "underlineColor": "primarySecond",
      "focusColor": "gray",
      "hoverColor": "gray"
    },
    "Link": {
      "activeForeground": "primarySecond"
    },
    "Table": {
      "stripeColor": "gray",
      "lightSelectionForeground": "white",
      "lightSelectionInactiveForeground": "white",
      "lightSelectionBackground": "gray",
      "lightSelectionInactiveBackground": "gray"
    }
  }
}